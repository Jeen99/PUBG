<Window x:Class="BattleRoyalClient.Queue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BattleRoyalClient"
        mc:Ignorable="d"
        Title="Поиск противника" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="60, 10, 0, 0">
            <TextBlock TextAlignment="Center" FontSize="18" Margin="5">Игроков в очереди</TextBlock>
            <TextBlock  x:Name="Players" FontSize="18" Margin="5" Width="100">0</TextBlock>
        </StackPanel>
        <Button x:Name="SignOut"  Width="220" Height="40" FontSize="20" Margin="10,20,10,0">Выйти</Button>
    </StackPanel>
</Window>
